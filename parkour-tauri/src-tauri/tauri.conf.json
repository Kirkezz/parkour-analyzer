{
  "$schema": "https://raw.githubusercontent.com/nicbarker/tauri-v2/main/packages/cli/schema.json",
  "productName": "Parkour Analyzer",
  "version": "1.0.0",
  "identifier": "dev.kirkezz.parkour-analyzer",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Parkour Analyzer",
        "width": 960,
        "height": 720,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
